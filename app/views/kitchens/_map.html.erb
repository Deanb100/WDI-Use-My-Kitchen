<style>
  #map {
    height: 400px;
    width: 100%;
   }
</style>


<div id="map"></div>

<script>
  initMap();
  var map;
  function drawMarkers(kitchens) {
    console.log('map', map);
    console.log('pins', kitchens);
    for (i=0; i < kitchens.length; i++ ) {
      var marker = new google.maps.Marker({
        position: {lat: kitchens[i].latitude, lng: kitchens[i].longitude},
        map: map
      });
    }
  }

  function initMap() {
    // TODO instead of center on melbourne get GPS cordinates of the user's browser
    var melbourne = {lat: -37.813628, lng: 144.963053};
    map = new google.maps.Map(document.getElementById('map'), {
      zoom: 16,
      center: melbourne
    });
  }
</script>
